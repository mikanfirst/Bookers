<h1>Books</h1>
<% @books.each do|book| %>
  <p>title</p>
  <span><%= book.title %></span>
  <p>Body</p>
  <span><%= book.body %></span>
  <%= link_to "destroy", book_path(book.id), method: :delete %>
  <%= link_to "edit", edit_book_path(book.id) %>
  <%= link_to "show", book_path(book.id) %>
<% end %>